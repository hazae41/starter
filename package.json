{
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "examine": "deno lint ./src && deno check ./src && deno test ./src",
    "version": "deno -RW ./x.vsync.ts && git add deno.json",
    "prepare": "deno -RW ./x.dsync.ts && deno install",
    "prepack": "glace ./src/mods/www --out=./out",
    "produce": "glace ./src/mods/www --out=./out --watch & deno -RNE ./src/mods/srv/bin.ts",
    "develop": "glace ./src/mods/www --out=./out --dev --watch & deno -RNE ./src/mods/srv/bin.ts --dev"
  },
  "files": [
    "./out"
  ],
  "dependencies": {
    "@hazae41/immutable": "^2.0.15",
    "@hazae41/labase": "^1.0.15",
    "@hazae41/rewind": "^2.3.12",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@hazae41/glace": "^1.0.13",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2"
  }
}