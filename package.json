{
  "type": "module",
  "name": "@hazae41/starter",
  "description": "Start a cool webapp with React and Tailwind",
  "version": "1.0.0",
  "repository": "github:hazae41/starter",
  "author": "hazae41",
  "scripts": {
    "examine": "deno lint ./src && deno check ./src && deno test ./src",
    "version": "deno -RW ./x.vsync.ts && git add deno.json",
    "prepare": "deno -RW ./x.dsync.ts && deno install",
    "prepack": "glace ./src/mods/www --out=./out",
    "produce": "glace ./src/mods/www --out=./out && deno -RN ./src/mods/server/bin.ts",
    "develop": "glace ./src/mods/www --out=./out --dev --watch & deno -RN ./src/mods/server/bin.ts"
  },
  "files": [
    "./out"
  ],
  "dependencies": {
    "@hazae41/immutable": "^2.0.15",
    "@hazae41/rewind": "^2.3.12",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@hazae41/glace": "^1.0.9",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2"
  }
}
