{
  "type": "module",
  "name": "@hazae41/starter",
  "description": "Just build a cool website",
  "version": "1.0.0",
  "repository": "github:hazae41/starter",
  "author": "hazae41",
  "scripts": {
    "examine": "deno lint ./src && deno check ./src && deno test ./src",
    "prepare": "glace ./src/mods/www --out=./out",
    "develop": "glace ./src/mods/www --out=./out --dev --watch & serve ./out",
    "version": "deno -RW ./x.vsync.ts && git add deno.json",
    "synchro": "deno -RW ./x.dsync.ts && deno install"
  },
  "files": [
    "./out"
  ],
  "dependencies": {
    "@hazae41/rewind": "^2.3.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@hazae41/glace": "^1.0.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "serve": "^14.2.5"
  }
}